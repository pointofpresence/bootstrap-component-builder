<% _.each(icons, function(item) { %>

    <a href="#" class="btn btn-default"
       data-toggle="tooltip" data-placement="bottom"
       title="<% print(item ? "glyphicon-" + item : "Remove Icon"); %>">
        <span class="glyphicon <% print(item ? "glyphicon-" + item : "empty-icon"); %>"></span>
    </a>

<% }); %>